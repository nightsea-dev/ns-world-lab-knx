{
  "name": "@ns-lab-klx/source",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "serve:board": "npm run clean && nx serve board --host=lab36 --watch",
    "build:board": "cls && nx build board --watch",
    "serve:admin": "cls && nx serve admin --host=lab36 --watch",
    "build:admin": "cls && nx build admin --watch",
    "build:all": "cls && nx run-many -t build --watch",
    "---------- tsc": "----------------------------------------",
    "clean": "tsc -b tsconfig.build.json --clean",
    "b:types:w": "tsc -b packages/types -w",
    "b:logic:w": "tsc -b packages/logic -w",
    "b:ui:w": "tsc -b packages/ui -w",
    "b:board:w": "tsc -b apps/board -w",
    "b:admin:w": "tsc -b apps/admin -w",
    "---------- nx": "----------------------------------------",
    "nx:types:w": "cls && nx run-many -t build --projects=types --verbose",
    "nx:logic:w": "cls && nx run-many -t build --projects=logic --verbose",
    "nx:ui:w": "cls && nx run-many -t build --projects=ui --verbose",
    "nx:board:w": "cls && nx run-many -t build --projects=board --verbose",
    "nx:admin:w": "cls && nx run-many -t build --projects=admin --verbose",
    "nx:graph": "cls && nx graph --watch --host=lab36 --port=4300"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@faker-js/faker": "^10.0.0",
    "mobx": "^6.13.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-rnd": "^10.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/jest": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/react": "21.5.2",
    "@nx/rspack": "21.5.2",
    "@nx/workspace": "21.5.2",
    "@rspack/cli": "^1.5.0",
    "@rspack/core": "^1.5.0",
    "@rspack/dev-server": "^1.1.4",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "nx": "21.5.2",
    "prettier": "^2.6.2",
    "react-refresh": "~0.14.0",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}