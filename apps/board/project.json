{
    "name": "@ns-lab-klx/board",
    "targets": {
        "serve": {
            "continuous": true,
            "dependsOn": [
                "^build"
            ],
            "options": {
                "cwd": "apps/board",
                "args": [
                    "--node-env=development"
                ],
                "env": {},
                "command": "rspack serve"
            },
            "syncGenerators": [
                "@nx/js:typescript-sync"
            ],
            "executor": "nx:run-commands",
            "configurations": {},
            "parallelism": true
        }
    }
}